{
    "name": "flyingcolours/hubspot-consumer",
    "description": "PHP library to consume Hubspot API",
    "type": "library",
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "require": {
        "php": ">=7.1",
        "kriswallsmith/buzz": "0.15.2",
        "monolog/monolog": "^1.23",
        "psr/simple-cache": "^1.0",
        "sabre/cache": "^1.0",
        "symfony/event-dispatcher": "^3",
        "symfony/http-kernel": "^3",
        "symfony/property-access": "^3",
        "symfony/property-info": "^3",
        "symfony/serializer": "^3"
    },
    "require-dev": {
        "leanphp/phpspec-code-coverage": "^4.0",
        "satooshi/php-coveralls": "^1.0",
        "phpspec/phpspec": "^4.2"
    },
    "config": {
        "bin-dir": "bin"
    },
    "license": "MIT"
}
